#01 23 * * * /usr/sbin/DMRIDUpdate.sh
#05 23 * * * /usr/sbin/BMMasterUpdate.sh
#06 23 * * * /usr/sbin/YSFHostsUpdate.sh
00 02 * * Mon,Tue,Wed,Thu,Fri,Sat,Sun /usr/bin/wifi_schedule.sh start
00 06 * * Mon,Tue,Wed,Thu,Fri,Sat,Sun /usr/bin/wifi_schedule.sh stop
00 5 * * 5 sleep 5 && touch /etc/banner && reboot
